# Example configuration for BPA integration stub
BPA_URL=
BPA_API_KEY=
BPA_AUTH_TYPE=Bearer

# Optional ML species classifier (ONNX)
# If not provided, a heuristic will be used.
SPECIES_ONNX_PATH=
SPECIES_LABELS_PATH=
SPECIES_INPUT_SIZE=224
SPECIES_USE_CROP=true
SPECIES_TTA=true
SPECIES_USE_LETTERBOX=true
SPECIES_USE_CLAHE=true
# Requires `pip install rembg` to enable background removal cropping
SPECIES_USE_REMBG=false
